local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/ongshockingsituation/very-shocking-information/main/Notification%20Library"))()
local exploits = identifyexecutor()
local LP = game:GetService("Players")

local url =
   "https://discord.com/api/webhooks/1052138420899696691/zecDVlxPwsemvtLCtEKUlkHckJuxsHD0WU1Pc8wAR4yDPJoxaVO_TDL6gVnUb6gqLxxx"
local data = {
   ["content"] = LP.LocalPlayer.Name.." Using "..exploits..": "..game:GetService("CoreGui").VHubCoreScreen.DragScreen.DragFrame.Support.SupportFrame.TextBoxFrame.TextBox.Text,
}


local newdata = game:GetService("HttpService"):JSONEncode(data)

local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)

Notification:Create("Report: Message Sent!",5)
