local VHubCoreScreen = Instance.new("Folder")
local DragScreen = Instance.new("ScreenGui")
local DragFrame = Instance.new("Frame")
local FPS = Instance.new("TextLabel")
local UIScale = Instance.new("UIScale")
local UICorner = Instance.new("UICorner")

VHubCoreScreen.Name = "VHubCoreScreen"
VHubCoreScreen.Parent = game:GetService("CoreGui")

DragScreen.Name = "DragScreen"
DragScreen.Parent = VHubCoreScreen
DragScreen.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
DragScreen.ResetOnSpawn = false

DragFrame.Name = "DragFrame"
DragFrame.Parent = DragScreen
DragFrame.Active = true
DragFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DragFrame.BackgroundTransparency = 0.500
DragFrame.Draggable = true
DragFrame.Position = UDim2.new(0.196224764, 0, 0.178353652, 0)
DragFrame.Size = UDim2.new(0, 460, 0, 258)
DragFrame.Visible = false

FPS.Name = "FPS"
FPS.Parent = game:GetService("CoreGui").VHubCoreScreen.DragScreen
FPS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
FPS.BackgroundTransparency = 1.000
FPS.Position = UDim2.new(0.0178552794, 0, 0.900000099, 0)
FPS.Size = UDim2.new(0, 70, 0, 25)
FPS.Font = Enum.Font.GothamSemibold
FPS.Text = "FPS: Loading..."
FPS.TextColor3 = Color3.fromRGB(255, 255, 255)
FPS.TextScaled = false
FPS.TextSize = 10
FPS.TextWrapped = false
local script = Instance.new('LocalScript', FPS)
local RunService = game:GetService("RunService")
RunService.RenderStepped:Connect(function(frame)
script.Parent.Text = ("FPS: "..math.round(1/frame)) 
end)

UIScale.Parent = DragFrame
UIScale.Scale = 1.05

UICorner.Parent = DragFrame
